<div class="limiter">
  <div class="container-login100">
    <div class="wrap-login100" style="display: contents;">
      <form class="login100-form validate-form" method="POST">
        <span class="login100-form-title p-b-43">
          Đăng kí
        </span>
        <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
          <input onfocusout="checkExistedUsername()" id="username-box" class="input100" type="text" name="username">
          <span class="focus-input100"></span>
          <span class="label-input100">Tên đăng nhập</span>
        </div>
        <span id= "usercheck-notify"></span>
        <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
          <input onfocusout="checkExistedEmail()" id="email-box" class="input100" type="text" name="email">
          <span class="focus-input100"></span>
          <span class="label-input100">Email</span>
        </div>
        <span id = "emailcheck-notify"></span>
        <div class="wrap-input100 validate-input" data-validate="Password is required">
          <input onkeyup="checkPassword()" id="password-box" class="input100" type="password" name="password">
          <span class="focus-input100"></span>
          <span class="label-input100">Mật khẩu</span>
        </div>
        <span id= "passwordcheck-notify"></span>
        <div class="wrap-input100 validate-input" data-validate="Password is required">
          <input onkeyup="checkRepassword()" id="repassword-box" class="input100" type="password" name="repassword">
          <span class="focus-input100"></span>
          <span class="label-input100">Xác nhận mật khẩu</span>
        </div>
        <span id= "repasswordcheck-notify"></span>
        <div class="container-login100-form-btn">
          <button disabled="true" style="background-color: black;" id="register-button" type="submit" class="login100-form-btn">
            Đăng kí
          </button>
        </div>
        <div class="text-center p-t-46 p-b-20">
          <span class="txt2">
            Hoặc đăng kí với
          </span>
        </div>
        <div class="login100-form-social flex-c-m">
          <a href="#" class="login100-form-social-item flex-c-m bg1 m-r-5">
            <i class="fab fa-facebook-f"></i>
          </a>
          <a href="#" class="login100-form-social-item google flex-c-m bg2 m-r-5">
            <i class="fab fa-google-plus-g"></i>
          </a>
        </div>
      </form>
    </div>
  </div>
</div>
